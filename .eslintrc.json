{
  "root": true,
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "@vue/typescript/recommended",
    "prettier"
  ],
  "plugins": [],
  "overrides": [
    {
      "files": ["pages/**/*.vue", "layouts/**/*.vue"],
      "rules": {
        "vue/multi-word-component-names": 0
      }
    }
  ]
}
